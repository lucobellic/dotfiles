    "custom/cliphist": {
        "format": "{}",
        "rotate": ${r_deg},
        "exec": "echo ; echo 󰅇 clipboard history",
        "on-click": "sleep 0.1 && cliphist.lua c",
        "on-click-right": "sleep 0.1 && cliphist.lua d",
        "on-click-middle": "sleep 0.1 && cliphist.lua w",
        "interval" : 86400, // once every day
        "tooltip": true
    },

